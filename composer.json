{
    "name": "oldpak/annotations",
    "description": "Docblock Annotations Parser",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "annotations",
        "docblock",
        "parser"
    ],
    "authors": [
        {
            "name": "oldpak",
            "email": "2233466866@qq.com"
        }
    ],
    "homepage": "https://www.doctrine-project.org/projects/annotations.html",
    "require": {
        "php": "^7.1 || ^8.0",
        "ext-tokenizer": "*",
        "oldpak/lexer": "^1.0",
        "psr/cache": "^1 || ^2 || ^3"
    },
    "require-dev": {
        "oldpak/cache": "^1.0",
        "oldpak/coding-standard": "^1.0",
        "phpstan/phpstan": "~1.4.10 || ^1.8.0",
        "phpunit/phpunit": "^7.5 || ^8.5 || ^9.5",
        "symfony/cache": "^4.4 || ^5.4 || ^6",
        "vimeo/psalm": "^4.10"
    },
    "suggest": {
        "php": "PHP 8.0 or higher comes with attributes, a native replacement for annotations"
    },
    "autoload": {
        "psr-4": {
            "Doctrine\\Common\\Annotations\\": "lib/Doctrine/Common/Annotations"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Doctrine\\Performance\\Common\\Annotations\\": "tests/Doctrine/Performance/Common/Annotations",
            "Doctrine\\Tests\\Common\\Annotations\\": "tests/Doctrine/Tests/Common/Annotations"
        },
        "files": [
            "tests/Doctrine/Tests/Common/Annotations/Fixtures/functions.php",
            "tests/Doctrine/Tests/Common/Annotations/Fixtures/SingleClassLOC1000.php"
        ]
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "sort-packages": true
    }
}
